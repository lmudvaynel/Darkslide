<!DOCTYPE html>
<html>
<head>
  <%= raw seo_meta_tags %>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
	<div id="wrapper">

		<%= render 'shared/header' %>

		<div id="center">
			<div class="page-wrapper">
				<div class="page">
					<div class="wrapper">

						<%= yield %>

					</div>
				</div>
			</div>
		</div>
	</div>

	<%= render 'shared/footer' %>
	
</body>
</html>
